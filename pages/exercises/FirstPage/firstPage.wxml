    </button>

    <button class='btn' bindtap="Button2">
       <image class='btn2Img' src='/static/main_history.png'></image>
       <view class='view-text'>
          <text class='text-content'>{{text1}}</text>
      </view>
       <!--<text class='text1'>{{text1}}</text>-->
       <!--<view>嗅味层次分析法训练</view>-->
    </button>
    
  </view>
  <view class="smell-row">

  <span style="font-size:18rpx;"><view>
    <button class='btn' bindtap="Button3">
       <image class='btn3Img' src='/static/main_upload_data.png'></image>
      <view class='view-text'>
          <text class='text-content'>{{text2}}</text>
      </view>
      <!--<text class='text1'>{{text2}}</text>-->
      <!--<view>嗅味投诉上报嗅味特征评价</view>-->
