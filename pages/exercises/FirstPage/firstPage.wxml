<!--pages/exercises/FirstPage/firstPage.wxml-->

<view class='smell-list'>
  <view class="smell-row">
    <button class='btn' bindtap="Button1">
       <image class='btn1Img' src='/static/main_test.png'></image>
       <text class='text'>{{text}}</text>
